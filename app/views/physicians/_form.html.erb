<%= form_for @physician do |f| %>
	<p>
		<%= f.label :name %><br>
		<%= f.text_field :name %>
	</p> 
	<p>
		<%= f.label :email %><br>
		<%= f.text_field :email %>
	</p> 
	<p>
		<%= f.label :password %><br>
		<%= f.password_field :password %>
	</p> 
	<p>
		<%= f.label :department %><br>
		<%= f.text_field :department %>
    </p> 
	<p>
		<%= f.submit %>
	</p>
<% end %>
